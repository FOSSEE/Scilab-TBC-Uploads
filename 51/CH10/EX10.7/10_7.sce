clc;
clear;
S0=1/500;
n1=0.02;
z1=0.6;//ft
n2=0.015;
n3=0.03;
z2=0.8;//ft
l1=3;//ft
l2=2;//ft
l3=3;//ft
y=z1+z2;//ft
K=1.49;
A1=l1*(z1);//ft^2
A2=l2*(y);//ft^2
A3=l3*(z1);//ft^2
P1=l1+z1;//ft
P2=l2+(2*z2);//ft
P3=l3+z1;//ft
Rh1=A1/P1;//ft
Rh2=A2/P2;//ft
Rh3=A3/P3;//ft
Q=K*(S0^0.5)*((A1*(Rh1^(2/3))/n1)+(A3*(Rh3^(2/3))/n3)+(A2*(Rh2^(2/3))/n2));//(ft^3)/sec
disp("(ft^3)/sec",Q,"The flowrate=")  
