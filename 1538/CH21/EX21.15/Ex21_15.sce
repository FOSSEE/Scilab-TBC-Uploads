//given
//length of wire
l=250*10^-3  //m
//no of turns
N=400
//current
I=15  //A
//permeability in vaccum
mu0=1.2457*10^-6  //H/m
//relative permeability
mur=1
//magnetic field strength 
H=N*I/l  //AT/m
//flux density is
B=mu0*mur*H  //Wb/m^2
printf ("the magnetic field strength is %f AT/m and flux density is %f Wb/m",H,B)
