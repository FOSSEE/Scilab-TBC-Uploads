x_rms=1*10^-2;//Root-mean-square distance (m)
D=6.7*10^-10;//Diffusion constant for glucose molecule in water (m^2/s)
t=x_rms^2/(2*D);//Time (s)
t=t/(60*60);//Time (h)
printf('Time taken for a glucose molecule to move 1 cm in water = %0.2f h',t)
