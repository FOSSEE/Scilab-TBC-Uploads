


                      // Example  5.4

mo=4*%pi*10^-7;     // Permeability of free Space
i1=4;               // Current in 1st Wire
i2=6;               // Current in 2nd Wire
r=0.03;             // Distance between 2 wires

F=(mo*i1*i2)/(2*%pi*r);
l=0.15;          // Section of wire
Fnet=F*l;
disp(' Force on 15 cm of wire B is = '+string(Fnet)+' N');





