// Additional solved examples , Example 13 , pg 336
m=0.05//mass(in Kg)
v=2000//speed(in m/sec)
h=6.625*10^-34//plancks constant(in J s)
p=m*v//momentum(in kg m/sec)
lam=h/p //wavelength
printf("de Broglie wavelength(in m)\n")
disp(lam)
printf("de Broglie wavelength(in A)\n")
disp(lam*10^10)
