clear;
close;
clc;
V=78e3;
Vph=V/sqrt(3);
Em=2*Vph;
pf=0.4;
angle=acos(pf);
k1=sin(angle); 
k1=round(k1*100)/100;
k2=.951;
k3=1;
k=k1*k2*k3;
k=round(k*1000)/1e3;
E=k*Em;
f=15000; 
t=1/(2*f);
t=round(t*1e6);
eavg=2*E/t;
eavg=round(eavg/100)*100;
printf("average restriking voltage=%fkV/microsecs",eavg/1e3);
