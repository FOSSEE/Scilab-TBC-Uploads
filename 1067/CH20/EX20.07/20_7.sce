clear;
clc;
r1=20e6;
rb=30e6;
v1=11e3;
v2=110e3;
x1g=.2*rb/r1;
x1t=.08*rb/r1;
x2g=.2;
x2t=.1;
xl=.516;
x0=xl/2;
x1=x1g+x1t;
x2=x2g+x2t;
x=inv(inv(x2)+inv(x1));
z=x+x0;
E=1;
isc=E/z;
ig1=isc*x2/(x1+x2);
ig2=isc*x1/(x1+x2);
i=ig1+ig2;
ib=rb/(1.7355*v1);
ig1=fix(ig1*1000)/1000;
Ig1=ig1*ib;
ib=fix(ib);
ig2=fix(ig2*100)/100;
Ig2=ig2*ib;
Ig2=fix(Ig2);
mprintf("the current taken from G1=%dA(lagging)\n the current taken from G2=%dA(lagging)",Ig1,Ig2);
