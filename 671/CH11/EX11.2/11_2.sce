P=45000
V=440
pf=0.8
Ia=P/sqrt(3)/V*exp(%i*acos(pf))
Vt=V/sqrt(3)
Rs=0.2
Xs=1.8
Ef=norm(Vt-Ia*(Rs+%i*Xs))
If=Ef/85
Pein=pf*P
Rf=35
Fl=If*If*Rf
Pin=Pein+Fl
Pcu=norm(3*Ia*Ia*Rs)
Pshaft=Pein-Pcu
Prl=1500
Pshaft_net=Pshaft-Prl
effi=Pshaft_net/Pin

disp(Pshaft_net)
disp(If)
disp(effi)
