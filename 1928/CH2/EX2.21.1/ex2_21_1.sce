ro=1.72*10^-8                           //resistivity of Cu

s=1/ro                                  //conductivity of Cu

n=10.41*10^28                           //no of electron per unit volume

e=1.6*10^-19                            //charge on electron

u=s/(n*e)

printf("mobility of electron in Cu =")

disp(u)

printf("m^2/volt-sec")