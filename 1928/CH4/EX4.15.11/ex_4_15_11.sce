t=1*10^-3                               //thicknesss of crystal

d=2.65*10^3                             //density

Y=8*10^10                               //Young's modulus

k=1                                     //consider 1st harmonic

n=(k/(2*t))*sqrt(Y/d)                   //formula of natural frequency

printf(" natural frequency =")

disp(n)

printf("Hz")
