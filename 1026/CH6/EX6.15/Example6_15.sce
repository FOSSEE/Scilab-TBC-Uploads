V1=1.4

I1=60*10^-3

V2=1.5

I2=85*10^-3

Rs1=V1/I1

Rs2=V2/I2

dV=V2-V1

dI=I2-I1

Rd=dV/dI

printf("static resistance\n")

printf("Rs1=%.2f ohm\n",Rs1)

printf("Rs2=%.2f ohm\n",Rs2)

printf("dynamic resistance\n")

printf("Rd=%.2f ohm",Rd)
