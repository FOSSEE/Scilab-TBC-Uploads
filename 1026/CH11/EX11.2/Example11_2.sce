lam=0.2865*10^-10//wavelength

mp=1.67*10^-27//mass of proton

h=6.625*10^-34

v=(h/(mp*lam))//debroglie's equation

KE=0.5*mp*(v^2)//kinetic energy of proton(J)

KE=KE/(1.6*10^-19)//converting into ev

printf("kinetic energy of proton\n")

printf("KE=%.2f ev",KE)