h=6.634*10^-34

c=3*10^8

e=1.6*10^-19

m=9.1*10^-31

Ep=100*10^3*e//energy of photon

lamp=((h*c)/Ep)//wavelength of photon

lame=lamp//wavelength of electron

v=h/(m*lame)

KEe=0.5*m*(v^2)//kinetic energy of electron

KEe=KEe/(1.6*10^-19)

printf("kinetic energy of electron\n")

printf("KEe=%.2f ev",KEe)