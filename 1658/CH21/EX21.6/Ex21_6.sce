clc;
VCC=10;
RB=470*10**3;
RE=1*10**3;
RL=1*10**3;
a=4;
B=50;
IE=VCC/(RE+(RB/B));
re=25/(IE*10**3);
Ri1=(RB*(B*re))/(RB+(B*re));
disp('ohm',Ri1*1,"Ri1=");
Ri2=(RB*(B*re))/(RB+(B*re));
disp('ohm',Ri2*1,"Ri2=");
RI2=(a^2)*Ri2;
RO1=RI2;
RI2=(a^2)*RL;
Av1=RO1/re;
disp(Av1);
RO2=RI2;
Av2=RO2/re;
disp(Av1);
Av=Av1*Av2;
disp(Av);
Gv=20*log10(Av);
disp('dB',Gv*1,"Gv=");
