clc;
VS=10*10**-3;
a=0.98;
VBE=0.7;
VCC=10;
RC=10*10**3;
RL=5.1*10**3;
RE=20*10**3;
VEE=10;
IE=(VEE-VBE)/RE;
re=25/IE*10**-3;
Ri=re;
Ris=(RE*re)/(RE+re);
disp('ohm',Ris,"Ris=");
Ai=a;
disp(Ai);
rL=(RC*RL)/(RC+RL);
Av=rL/re;
disp(Av);
Ap=Av*Ai;
disp(Ap);
Gp=10*log10(Ap);
disp('dB',Gp,"Gp=");
Vin=VS;
Vo=Av*Vin;
disp('mV',Vo*10**3,"Vo=");
