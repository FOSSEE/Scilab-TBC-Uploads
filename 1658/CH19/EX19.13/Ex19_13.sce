clc;
B=80;
VBE=0.7;
VCC=15;
R1=20*10**3;
R2=20*10**3;
RS=2*10**3;
VS=5*10**-3;
RE=8.2*10**3;
RL=1.5*10**3;
VTH=VCC*R2/(R1+R2);
RTH=(R1*R2)/(R1+R2);
IE=(VTH-VBE)/(RE+(RTH/B));
disp('mA',IE*10**3,"IE=");
re=25/IE*10**-3;
rL=(RE*RL)/(RE+RL);
Ri=B*(rL+re);
Ris=(RTH*Ri)/(RTH+Ri);
disp('kohm',Ris*10**-3,"Ris=");
Ro=re+((RS*RTH)/(RS+RTH))/B;
disp('ohm',Ro,"Ro=");
Vin=VS*Ris/(RS+Ris);
disp('mV',Vin*10**3,"Vin=");
