clc;
RC=4*10**3;
RB=40*10**3;
RS=10*10**3;
hie=1100;
hfe=50;
hre=0;
hoe=0;
RB2=40*10**3;
rL=(RC*RB2)/(RC+RB2);
Ai=-hfe/(1+hoe*rL);
Ri=hie+hre*Ai*rL;
Av=(Ai*rL)/Ri;
RB1=40*10**3/(1-Av);
Ris=(Ri*RB1)/(Ri+RB1);
disp('ohm',Ris*1,"Ris=");
Ros=rL;//Ro=infinity
disp('Ohm',Ros*1,"Ros=");
Avs=(Av*Ris)/(RS+Ris);
disp(Avs);
