clc;
hie=1.0*10**3;
hre=1*10**-4;
hoe=100*10**-6;
RC=1000;
RS=1000;
rL=RC;
hfe=50;
Ai=-hfe/(1+hoe*rL);
Ri=hie+hre*Ai*rL;
Ris=Ri;
disp('Ohm',Ris*1,"Ris=");
delh=hie*hoe-hre*hfe;
his=1000;
Ro=(RS+his)/(RS*hoe+delh);
disp('kOhm',Ro*10**-3,"Ro=");
Ros=(Ro*rL)/(Ro+rL);
disp('Ohm',Ros*1,"Ros=");
Ais=(Ai*RS)/(RS+Ris);
disp(Ais);
Av=(Ai*rL)/Ri;
Avs=(Av*Ris)/(RS+Ris);
disp(Avs);
