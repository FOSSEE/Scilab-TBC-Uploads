clc;
hic=2*10**3;
hfc=-51;
hrc=1;
hoc=25*10**-6;
rL=5*10**3;
RE=5*10**3;
Rs=1000;
R1=10*10**3;
R2=10*10**3;
Ai=-hfc/(1+hoc*rL);
disp(Ai);
Ri=hic+hrc*Ai*rL;
disp('kOhm',Ri*10**-3,"Ri=");
a=(R1*R2)/(R1+R2);
Ris=(Ri*a)/(Ri+a);
disp('Ohm',Ris*1,"Ris=");
Ro=-(Rs+hic)/hfc;
Ros=(Ro*RE)/(Ro+RE);
disp('Ohm',Ros*1,"Ros=");
Ais=(Ai*Rs)/(Rs+Ris);
disp(Ais);
Av=(Ai*rL)/Ri;
disp(Av);
Avs=(Av*Ris)/(Rs+Ris);
disp(Avs);
