clc;
VCC=20;
VEE=20;
RC=5*10**3;
RE=10*10**3;
RB=10*10**3;
B1=50;
B2=100;
VBE1=0.7;
VBE2=0.6;
IE1=(VEE-VBE1)/(RE+RB/B1);
disp('mA',IE1*10**3,"IE1=");
IC1=IE1;
VC1=VCC-IC1*RC;
disp('V',VC1,"VC1=");
VE=-VBE1;
VCE1=VC1-VE;
disp('V',VCE1,"VCE1=");
IE2=(VEE-VBE2)/(RE+RB/B2);
disp('mA',IE2*10**3,"IE2=");
IC2=IE2;
VC2=VCC-IC2*RC;
disp('V',VC2,"VC2=");
VE=-VBE2;
VCE2=VC2-VE;
disp('V',VCE2,"VCE2=");
delIc=(IC2-IC1)/IC1;
disp('%',delIc*100,"delIc=");
delVCE=(VCE1-VCE2)/VCE2;
disp('%',delVCE*100,"delVCE=");