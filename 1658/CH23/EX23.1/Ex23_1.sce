clc;
ID=4*10**-3;
IDSS=2*ID;
RS=390;
VGSQ=-ID*RS;
VP=-4.5;
RD=2.2*10**3;
gm0=(2*IDSS)/(-VP);
gm=gm0*(1-(VGSQ/VP));
Av1=-gm*RD;
Av2=-gm*RD;
Av=Av1*Av2;
disp(Av);
vi=20*10**-3;
vo=Av*vi;
disp('mV',vo*10**3,"vo=");
Zi=10*10**6;
RG=10*10**6;
disp('Mohm',Zi*10**-6,"Zi=RG=");
Z0=2.2*10**3;
RD=2.2*10**3;
disp('Kohm',Z0*10**-3,"Z0=RD=");
RL=10*10**3;
VL=(RL/(Z0+RL))*vo;
disp('V',VL*10**3,"VL=");
