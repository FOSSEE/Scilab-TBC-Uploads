clear;
clc;
format('v',6);
Q1=1;
Q2=-2;
Q=10*10^-9;
P1=[0 3 1]-[3 2 -1];
P2=[0 3 1]-[-1 -1 4];

e=10^-9/(36*%pi);
det1=(P1(1,1)^2+P1(1,2)^2+P1(1,3)^2)^.5;
det2=(P2(1,1)^2+P2(1,2)^2+P2(1,3)^2)^.5;
F=[[(Q*Q1)*(P1)]/(4*%pi*e*(det1)^3)]+[[(Q*Q2)*(P2)]/(4*%pi*e*(det2)^3)];
E=[(10^-6)*(F/Q)];
disp(F,'F(in mN)=');
disp(E,'At that point E(in kV)=');