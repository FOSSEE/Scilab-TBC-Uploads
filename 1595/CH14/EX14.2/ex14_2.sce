c=3*10^8;//velocity of light
f=12*10^9;//frequency
D=4.5;//diameter of parabolic reflector
h=c/f;//wavelength
x=D/h;
R=(2*D^2)/h;
printf("\nThe wavelength, \nh= %.3f m/cycle",h);
printf("\nD=4.5 meter \nD/h = %.f",x);
printf("\nselect equation, \nR > %.f m",R);