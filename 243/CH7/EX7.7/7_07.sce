//Example No. 7_07
//Ill-Conditioned Systems
//Pg No. 245

clear ; close ; clc ;

A = [ 2  1 ; 2.001  1];
B = [ 25 ; 25.01 ];
x(1) = (25 - 25.01)/(2 - 2.001);
x(2) = (25.01*2 - 25*2.001)/(2*1 - 2.001*1);
printf('\n x(1) = %f \n x(2) = %f \n',x(1),x(2))
x(1) = (25 - 25.01)/(2 - 2.0005);
x(2) = (25.01*2 - 25*2.0005)/(2*1 - 2.0005*1);
printf('\n x(1) = %f \n x(2) = %f \n',x(1),x(2))
r = A*x-B
disp(x)
disp(r)
