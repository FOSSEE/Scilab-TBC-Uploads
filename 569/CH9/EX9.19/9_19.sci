// calculate attenuation
clc;
T=273+20;
P=101.3*10^3;
R=287;
de=P/(R*T);
C=20.04*T^0.5;
r=6.25*10^-3;
L=0.6;
V=%pi*[(12.5*10^-3)^2]*(12.5*10^-3);
wn=C*r*(%pi/(V*(L+0.5*%pi*r)))^0.5;
fn=wn/(2*%pi);
f=1000;
u=f/fn;
mu=19.1*10^-6;
eta=[2*mu/(de*C*r^3)]*[3*L*V/%pi]^0.5;
M=1/{[(1-u^2)^2]+[(2*eta*u)^2]}^0.5;
%M=M*100;
disp(%M,'attenuation=')
