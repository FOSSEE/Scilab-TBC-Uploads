// Variable declaration
Mean = 10       // mean: kilowatt-hours
std_dev = 3     // standard deviation: kilowatt-hours
cost = 20       // in $ per kilowatt-hours

// Calculation
m1 = Mean*cost
s1 = std_dev*cost
v1 = s1^2

// Result
printf ( "Mean: %d dollar",m1)
printf ( "Standard Deviation: %d dollar",s1)
printf ( "Variance: %d dollar",v1)
