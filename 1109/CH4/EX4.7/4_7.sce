Deq=nthroot(d1*d2*d3,3);
Cn2=0.02414/(log10(Deq/r))*(10^-6);
printf("(b)Capacitance per km =%f microfarads/km,to neutral\n",fix(Cn2*(10^6)*10^5)/10^5);
Ic2=Vn*2*%pi*f*Cn2;
printf("\tCharging current = %f mA/km",round(Ic2*(10^3)));
